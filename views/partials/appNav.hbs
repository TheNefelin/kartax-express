<nav>
    <section class="navBar-contenedor">
        <a href="#" class="navBar-logo">
            {{#each katrax}}
            <img src={{link}}>
            <h1>{{nombre}}</h1>
            {{/each}}
        </a>
        <div class="navBar-btn">
            <div class="navBar-A"></div>
            <div class="navBar-B"></div>
            <div class="navBar-C"></div>
        </div>
        <div class="navBar-links">
            <a href="#">RRSS</a>
            <a href="/">Clientes</a>
            <a href="/iniciarSesion">Iniciar Sesi√≥n</a>
            <a href="/registrarse">Registrarse</a>
        </div>
    </section>
</nav>

<script>
    const linkContainer = document.querySelector(".navBar-links"); 

    const burger = document.querySelector(".navBar-btn");
    burger.addEventListener("click", () => {
        navBar();
    });

    const arrLinks = document.querySelectorAll(".navBar-links a");
    arrLinks.forEach(e => {
        e.addEventListener("click", () => {
            navBar();
        });
    });

    function navBar() {
        burger.classList.toggle("navBar-btn_click");
        linkContainer.classList.toggle("navBar-links_noOcultar");
    };
</script>